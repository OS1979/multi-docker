{
    "AWSEBDockerrunVersion": 2,
    "ContainerDefinition": [
      {
        "name": "client",
        "image": "817076261729.dkr.ecr.eu-central-1.amazonaws.com/multi-client/multi-client",
        "hostname": "client",
        "essential": false,
        "memory": 128
      },

      {
        "name": "server",
        "image": "817076261729.dkr.ecr.eu-central-1.amazonaws.com/multi-server/multi-server",
        "hostname": "api",
        "essential": false,
        "memory": 128
      },

      {
        "name": "worker",
        "image": "817076261729.dkr.ecr.eu-central-1.amazonaws.com/multi-worker/multi-worker",
        "hostname": "worker",
        "essential": false,
        "memory": 128
      },

      {
        "name": "nginx",
        "image": "817076261729.dkr.ecr.eu-central-1.amazonaws.com/multi-nginx/multi-nginx",
        "hostname": "nginx",
        "essential": true,
        "portMappings": [
            {
                "hostPort": "80",
                "containerPort": "80"
            }
        ],
        "links": ["client", "server"],
        "memory": 128
      }


    ]
}